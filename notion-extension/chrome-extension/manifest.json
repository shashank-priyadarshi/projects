{
    "manifest_version": 1,
    "version": "1.0",
    "name": "notesync",
    "content_script": [
        {
            "matches": [
                "<all_urls>"
            ]
        },
        {
            "script": [
                "main.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "pp.jpg"
    ],
    "icons": {
        "48": "icon-48.png",
        "96": "icon-96.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "icon-19.png",
            "38": "icon-38.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs"
    ]
}